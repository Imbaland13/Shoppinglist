@model IEnumerable<WebApplication1.Models.Product>
@{
	Layout = null;
}
<!DOCTYPE html>
<html>
	<head>
	<meta name="viewport" content="width=device-width" />
	<title>Shoppinglist</title>
	</head>
	<body>
		<table>
			<thead>
				<tr><th>Name</th><th>Number</th></tr>
			</thead>
			<tbody>
			@foreach (WebApplication1.Models.Product p in Model!)
			{
				<tr>
					<td>@p.Name</td>
					<td>@p.Number</td>
				</tr>
			}
			</tbody>
		</table>
	</body>
</html>
